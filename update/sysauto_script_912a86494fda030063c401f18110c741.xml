        <script><![CDATA[	// Query the database for NeedIt Task records 
	// Only return NeedIt  records that do not have
	// a State field value of Closed Complete.
	var overdueNITasks = new GlideRecord('x_58872_needit_needit');
	overdueNITasks.addQuery('state','!=',3);

	overdueNITasks.query();
	// Write a log message for each overdue NeedIt Task Record
	while(overdueNITasks.next()){
		gs.info("NeedIt  record = " + overdueNITasks.number);
		gs.eventQueue('x_58872_needit.requestedForRandomChange', overdueNITasks, overdueNITasks.number, gs.getUserName(), 'my_queue');
	}]]></script>
